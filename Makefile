#-------------------------------------------------------
# Choose a compiler
# #-------------------------------------------------------- 
CC = gcc 

# #--------------------------------------------------------
# # Set your source file and your target executable
# #--------------------------------------------------------
SRC = Project2_schung21_205.c
TARGET = Project2_schung21_205

all: $(TARGET)

# #--------------------------------------------------------
# # Compile the program
# #--------------------------------------------------------
$(TARGET):
	$(CC) -o $@ $(SRC)

# #--------------------------------------------------------
# # Clean up (remove the executable)
# #--------------------------------------------------------
clean:
	rm -f $(TARGET)
